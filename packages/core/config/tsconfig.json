{
    "display": "React Native",
    "compilerOptions": {
        "target": "es6",
        "module": "es2015",
        "types": [
            "react-native",
            "jest"
        ],
        "lib": [
            "es6",
            "es2019",
            "es2020.bigint",
            "es2020.date",
            "es2020.number",
            "es2020.promise",
            "es2020.string",
            "es2020.symbol.wellknown",
            "es2021.promise",
            "es2021.string",
            "es2021.weakref",
            "es2022.array",
            "es2022.object",
            "es2022.string"
        ],
        "jsx": "react-native",
        "resolveJsonModule": true,
        "declaration": true,
        "declarationMap": true,
        "sourceMap": true,
        "strict": true,
        "noImplicitAny": true,
        "moduleResolution": "node",
        "allowSyntheticDefaultImports": true,
        "plugins": [
            {
                "name": "@styled/typescript-styled-plugin",
                "tags": [
                    "styled",
                    "css",
                    "sty"
                ],
                "lint": {
                    "validProperties": [
                        "text-align-vertical",
                        "shadow-color",
                        "shadow-opacity",
                        "shadow-offset",
                        "shadow-radius",
                        "padding-horizontal",
                        "padding-vertical",
                        "margin-vertical",
                        "margin-horizontal",
                        "tint-color",
                        "aspect-ratio",
                        "elevation",
                        "resize-mode",
                        "include-font-padding"
                    ],
                    "unknownProperties": "error",
                    "float": "warning"
                }
            }
        ]
    }
}